{
  "name": "AWNET to MQTT",
  "version": "0.143",
  "slug": "awnet_to_mqtt",
  "description": "Addon to capture local Ambient Weather data from ObserverIP",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "homeassistant_api": true,
  "options": {
    "entity_id": "WS2902B",
    "publish_all_sensors": false
  },
  "schema": {
    "entity_id": "match(^[A-Za-z0-9]+)",
    "publish_all_sensors": "bool"
  },
  "ports": {
    "80/tcp": 7080
  }
}
